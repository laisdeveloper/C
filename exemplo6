#include <stdio.h>

int soma(int num1, int num2){
  int somafunc=0;
  if(num1<num2){
    for(int i=num1;i<=num2;i++){ somafunc+=i; }
    printf("%d",somafunc);
  }
  if(num1>num2){
    for(int i=num2;i<=num1;i++){ somafunc+=i; }
    printf("%d",somafunc);
  }
  if (num1==num2) printf("%d",num1+num2);
}

int main(void) {
  int n1,n2;
  scanf("%d",&n1);
  scanf("%d",&n2);
  soma(n1,n2);
  
  return 0;
}
